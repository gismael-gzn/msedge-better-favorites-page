{
  "manifest_version": 3,
  "name": "My Extension with Starting Page",
  "version": "1.0",
  "description": "An extension that opens a starting page.",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "bookmarks"]
}
